num = [1,2,3,4,5,6,7,8,9]
count_even = 0
count_odd = 0
for i in num:
    
                if i % 2 != 0:
                    count_odd = count_odd+1
                else:
                    count_even = count_even+1
print("Number of odd numbers :", count_odd)
print("Number of even numbers :", count_even)
